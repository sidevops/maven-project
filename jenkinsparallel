pipeline{
	
			agent any
			
			stages{
			
			
					stage ('scm checkout')
					
					{
						steps{
						
						echo 'deploy on vm1'
						}
					
					
					
					
					stage('parallel stage')
					
					parallel{
					
					stage ('scm checkout')
					
					{
						steps{
						
						echo 'deploy on vm2'
						}
					
					
					}
					
					stage ('scm checkout')
					
					{
						steps{
						
						echo 'deploy on vm2'
						}
					
					
					}
					}
					
			
			
			}
			}
			
	
	
